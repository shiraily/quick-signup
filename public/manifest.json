{
  "name": "Smart Autofill",
  "manifest_version": 3,
  "version": "0.2",
  "action": {
    "default_icon": { "32": "icon_32.png" },
    "default_title": "会員情報を自動入力"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "file://*"],
      "js": ["content_scripts.js"]
    }
  ],
	"background": {
		"service_worker": "background.js"
	},
  "permissions": ["storage"]
}
